<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC 
"-//mybatis.org//DTD Mapper 3.0//EN" 
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="sample">
	<select id="sampleList" resultType="kr.go.mohw.SampleDTO">
		select * from sample
	</select>
	<select id="getsample" resultType="kr.go.mohw.SampleDTO">
		select * from sample where id=#{id}
	</select>
	<insert id="addsample">
		insert into sample(id, pw) values #{id}, #{pw}
	</insert>
</mapper>
<!-- namespace나 해당 명령의 id는 중복되면 절대 안됨 -->